[32m+[m[32m  static List<PopularDestination> getDestinationsByCountry(String country) {[m
[32m+[m[32m    final destinations = getPopularDestinations();[m
[32m+[m[32m    if (country == 'T·∫•t c·∫£') return destinations;[m
[32m+[m[32m    return destinations.where((dest) => dest.country == country).toList();[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  static List<String> getAvailableCountries() {[m
[32m+[m[32m    final destinations = getPopularDestinations();[m
[32m+[m[32m    final countries = destinations.map((dest) => dest.country).toSet().toList();[m
[32m+[m[32m    return ['T·∫•t c·∫£', ...countries];[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  static List<PopularDestination> getPopularOnly() {[m
[32m+[m[32m    return getPopularDestinations().where((dest) => dest.isPopular == true).toList();[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  static List<PopularDestination> getTopRatedDestinations({int limit = 5}) {[m
[32m+[m[32m    final destinations = getPopularDestinations();[m
[32m+[m[32m    destinations.sort((a, b) => b.rating.compareTo(a.rating));[m
[32m+[m[32m    return destinations.take(limit).toList();[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  static List<PopularDestination> searchDestinations(String query) {[m
[32m+[m[32m    final destinations = getPopularDestinations();[m
[32m+[m[32m    return destinations.where((dest) =>[m[41m [m
[32m+[m[32m      dest.name.toLowerCase().contains(query.toLowerCase()) ||[m
[32m+[m[32m      dest.country.toLowerCase().contains(query.toLowerCase()) ||[m
[32m+[m[32m      dest.description.toLowerCase().contains(query.toLowerCase())[m
[32m+[m[32m    ).toList();[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  static List<PopularDestination> getDestinationsByRating(double minRating) {[m
