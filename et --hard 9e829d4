[32m+[m[32m  bool includeEntertainment;[m
[32m+[m
[32m+[m[32m  TripPlanData({[m
[32m+[m[32m    this.destination = 'H√† N·ªôi, Vi·ªát Nam',[m
[32m+[m[32m    this.departure = 'TP. H·ªì Ch√≠ Minh',[m
[32m+[m[32m    this.startDate = '13 Feb 2025',[m
[32m+[m[32m    this.endDate = '20 Feb 2025',[m
[32m+[m[32m    this.travelers = 2,[m
[32m+[m[32m    this.destinations = 1,[m
[32m+[m[32m    this.activitiesPerDay = 3,[m
[32m+[m[32m    this.budget = 5000000,[m
[32m+[m[32m    this.includeTransport = true,[m
