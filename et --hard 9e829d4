[32m+[m[32m  static List<TripPlan> filterTripsByStatus(List<TripPlan> trips, String filter) {[m
[32m+[m[32m    if (filter == 'T·∫•t c·∫£') return trips;[m
[32m+[m[41m    [m
[32m+[m[32m    TripStatus? filterStatus;[m
[32m+[m[32m    switch (filter) {[m
[32m+[m[32m      case 'Ho√†n th√†nh':[m
[32m+[m[32m        filterStatus = TripStatus.completed;[m
[32m+[m[32m        break;[m
[32m+[m[32m      case 'ƒêang th·ª±c hi·ªán':[m
[32m+[m[32m        filterStatus = TripStatus.ongoing;[m
[32m+[m[32m        break;[m
[32m+[m[32m      case 'K·∫ø ho·∫°ch':[m
[32m+[m[32m        filterStatus = TripStatus.planned;[m
[32m+[m[32m        break;[m
[32m+[m[32m      default:[m
[32m+[m[32m        return trips;[m
[32m+[m[32m    }[m
[32m+[m[41m    [m
[32m+[m[32m    return trips.where((trip) => trip.status == filterStatus).toList();[m
[32m+[m[32m  }[m
