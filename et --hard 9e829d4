[32m+[m
[32m+[m[32m  String get dateRange => '$startDate - $endDate';[m
[32m+[m[41m  [m
[32m+[m[32m  // T·ª± ƒë·ªông t√≠nh s·ªë ng√†y d·ª±a tr√™n startDate v√† endDate[m
[32m+[m[32m  int get totalDays {[m
[32m+[m[32m    try {[m
[32m+[m[32m      // T√≠nh to√°n ƒë∆°n gi·∫£n d·ª±a tr√™n format ng√†y[m
[32m+[m[32m      // V√≠ d·ª•: "13 Feb - 20 Feb 2025" -> 7 ng√†y[m
[32m+[m[32m      if (startDate.isNotEmpty && endDate.isNotEmpty) {[m
[32m+[m[32m        // Parse c∆° b·∫£n ƒë·ªÉ l·∫•y s·ªë ng√†y[m
[32m+[m[32m        final startDay = _extractDay(startDate);[m
[32m+[m[32m        final endDay = _extractDay(endDate);[m
[32m+[m[41m        [m
[32m+[m[32m        if (startDay > 0 && endDay > 0 && endDay >= startDay) {[m
[32m+[m[32m          return endDay - startDay + 1;[m
[32m+[m[32m        }[m
[32m+[m[32m      }[m
[32m+[m[32m      return 7; // Default value[m
[32m+[m[32m    } catch (e) {[m
[32m+[m[32m      return 7; // Default value[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m[41m  [m
[32m+[m[32m  // H√†m helper ƒë·ªÉ extract ng√†y t·ª´ string[m
[32m+[m[32m  int _extractDay(String dateStr) {[m
[32m+[m[32m    try {[m
[32m+[m[32m      // Extract s·ªë t·ª´ ƒë·∫ßu string, v√≠ d·ª•: "13 Feb" -> 13[m
[32m+[m[32m      final parts = dateStr.trim().split(' ');[m
[32m+[m[32m      if (parts.isNotEmpty) {[m
[32m+[m[32m        return int.tryParse(parts[0]) ?? 0;[m
[32m+[m[32m      }[m
[32m+[m[32m      return 0;[m
[32m+[m[32m    } catch (e) {[m
[32m+[m[32m      return 0;[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m[41m  [m
[32m+[m[32m  String get planSummary => '$travelers Ng∆∞·ªùi, $totalDays ng√†y, $activitiesPerDay ho·∫°t ƒë·ªông/ng√†y';[m
[32m+[m
